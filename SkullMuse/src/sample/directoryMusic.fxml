<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.DirectoryMusic" prefHeight="400.0" prefWidth="600.0">
    <children>
        <MenuBar fx:id="menubar" prefHeight="768.0" prefWidth="1024.0">
            <menus>
                <Menu fx:id="profil_dynamo" mnemonicParsing="false" text="Profile">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#pressMenuEditProfile" text="Edit my profile" />
                        <MenuItem mnemonicParsing="false" onAction="#pressMenuDisconnect" text="Disconnect" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Music">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#pressMenuDisplaySkull" text="Home" />
                        <MenuItem mnemonicParsing="false" onAction="#pressMenuDirectory" text="My directory" />
                        <MenuItem mnemonicParsing="false" onAction="#pressMenuUpload" text="Upload music" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <ScrollPane hbarPolicy="NEVER" layoutY="30" prefHeight="768.0" prefWidth="1024.0" vbarPolicy="ALWAYS">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="1000.0" prefWidth="1024.0">
                    <children>
                        <Label layoutX="14.0" layoutY="50.0" text="Your music added">
                            <font>
                                <Font name="Loma Bold" size="16.0" />
                            </font>
                        </Label>
                        <ScrollPane fx:id="display_muse_added" layoutX="12.0" layoutY="90.0" prefHeight="300.0" prefWidth="803.0" vbarPolicy="NEVER">
                            <content>
                                <GridPane fx:id="grid_hits_added" gridLinesVisible="true">
                                    <columnConstraints>
                                        <ColumnConstraints halignment="CENTER" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="Play" GridPane.halignment="CENTER" />
                                        <Label text="Song" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                                        <Label text="Artist" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                                        <Label text="Gender" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                                    </children>
                                </GridPane>
                            </content>
                        </ScrollPane>
                        <Label layoutX="14.0" layoutY="410.0" text="Your music liked">
                            <font>
                                <Font name="Loma Bold" size="16.0" />
                            </font>
                        </Label>
                        <ScrollPane fx:id="display_muse_liked" layoutX="12.0" layoutY="450.0" prefHeight="300.0" prefWidth="803.0" vbarPolicy="NEVER">
                            <content>
                                <GridPane fx:id="grid_hits_liked" gridLinesVisible="true">
                                    <columnConstraints>
                                        <ColumnConstraints halignment="CENTER" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="Play" GridPane.halignment="CENTER" />
                                        <Label text="Song" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                                        <Label text="Artist" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                                        <Label text="Gender" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                                    </children>
                                </GridPane>
                            </content>
                        </ScrollPane>
                        <Label layoutX="14.0" layoutY="760.0" text="Your music Heart Stroke">
                            <font>
                                <Font name="Loma Bold" size="16.0" />
                            </font>
                        </Label>
                        <ScrollPane fx:id="display_muse_heart" layoutX="12.0" layoutY="800.0" prefHeight="300.0" prefWidth="803.0" vbarPolicy="NEVER">
                            <content>
                                <GridPane fx:id="grid_heart_stroke" gridLinesVisible="true">
                                    <columnConstraints>
                                        <ColumnConstraints halignment="CENTER" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="Play" GridPane.halignment="CENTER" />
                                        <Label text="Song" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                                        <Label text="Artist" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                                        <Label text="Gender" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
                                    </children>
                                </GridPane>
                            </content>
                        </ScrollPane>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
